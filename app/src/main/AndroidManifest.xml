<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="azynias.study">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.KanjiDashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.KanjiTiersActivity" />
        <activity android:name=".Activities.KanjiDBActivity" />
        <activity android:name=".Activities.StudyActivity" />
        <activity android:name=".Activities.QuizActivity" />
        <activity android:name=".Activities.ProgressActivity" />
        <activity android:name=".Activities.StudyQuizActivity" />
        <activity android:name=".Activities.KanjiRecyclerViewActivity"></activity>
    </application>

</manifest>